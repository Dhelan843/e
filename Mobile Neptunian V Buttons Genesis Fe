end)

--==[ Delete Button (Removes all) ]==--
_0x2d("Delete Buttons", UDim2.new(1, -10, 0, 10), function()
    for _, _0x35 in ipairs(_0x9:GetChildren()) do
        if _0x35:IsA("TextButton") or _0x35:IsA("ImageButton") then
            _0x14(_0x35)
        end
    end
end)
